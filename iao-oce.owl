    <owl:Ontology rdf:about="http://www.merrellvajda.org/ontologies/commerce/oce.owl">
        <owl:imports rdf:resource="http://purl.obolibrary.org/obo/iao/d-acts/release/2019-01-30/d-acts.owl"/>
        <dc:creator xml:lang="en">Eric Merrell &lt;ericmerr@buffalo.edu&gt;</dc:creator>
        <dc:creator xml:lang="en">Jonathan Vajda &lt;jvajda@buffalo.edu&gt;</dc:creator>
        <dc:date xml:lang="en">Last Revision: 2023-03-22</dc:date>
        <dc:description xml:lang="en">A reference ontology for the commerce domain. Conformant with Basic Formal Ontology (BFO). Intended to be the top-level ontology for the commerce domain, to be imported by future ontologies for finance, retail, marketing, and economics domains.</dc:description>
        <owl:versionInfo xml:lang="en">0.0.4</owl:versionInfo>
    </owl:Ontology>
    


